<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\common.props" />
  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Reflection\MemberInvokeTest.cs" />
    <Compile Remove="Reflection\ReflectionInvokerTests.cs" />
    <Compile Remove="Serialization\CollectionTest.cs" />
    <Compile Remove="Serialization\SimpleTest.cs" />
  </ItemGroup>

  <ItemGroup>
    <ContentWithTargetPath Include="Resources\encrypted_config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <TargetPath>encrypted_config</TargetPath>
    </ContentWithTargetPath>
    <None Include="Resources\encrypted_config" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="$(MsTestSdkVer)" />
    <PackageReference Include="MSTest.TestAdapter" Version="$(MsTestVer)" />
    <PackageReference Include="MSTest.TestFramework" Version="$(MsTestVer)" />

    <PackageReference Include="Microsoft.Data.Sqlite" Version="$(MsExtensionsVer)" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="$(MicrosoftDataSqlClientVer)" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Compilation.Roslyn\Compilation.Roslyn.csproj" />
    <ProjectReference Include="..\Data.Linq2db\Data.Linq2db.csproj" />
    <ProjectReference Include="..\Interop.Windows\Interop.Windows.csproj" />
    <ProjectReference Include="..\IO\IO.csproj" />
    <ProjectReference Include="..\Net.Clients\Net.Clients.csproj" />
    <ProjectReference Include="..\Serialization\Serialization.csproj" />
    <ProjectReference Include="..\UnitTesting\UnitTesting.csproj" />
  </ItemGroup>
</Project>