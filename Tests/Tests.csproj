<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\common.props" />
  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="$(MsTestSdkVer)" />
    <PackageReference Include="MSTest.TestAdapter" Version="$(MsTestVer)" />
    <PackageReference Include="MSTest.TestFramework" Version="$(MsTestVer)" />
    <PackageReference Include="SixLabors.ImageSharp" Version="$(ImageSharpVer)" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.*" />
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Compilation.FSharp\Compilation.FSharp.csproj" />
    <ProjectReference Include="..\Compilation.Python\Compilation.Python.csproj" />
    <ProjectReference Include="..\Compilation.Roslyn\Compilation.Roslyn.csproj" />
    <ProjectReference Include="..\Backup\Backup.csproj" />
    <ProjectReference Include="..\Backup.AWS\Backup.AWS.csproj" />
    <ProjectReference Include="..\Backup.Azure\Backup.Azure.csproj" />
    <ProjectReference Include="..\Backup.Mega\Backup.Mega.csproj" />
    <ProjectReference Include="..\Backup.Yandex\Backup.Yandex.csproj" />
    <ProjectReference Include="..\Data.Linq2db\Data.Linq2db.csproj" />
    <ProjectReference Include="..\Interop.Excel\Interop.Excel.csproj" />
    <ProjectReference Include="..\Interop\Interop.csproj" />
    <ProjectReference Include="..\IO.Compression\IO.Compression.csproj" />
    <ProjectReference Include="..\Logging\Logging.csproj" />
    <ProjectReference Include="..\Net.Clients\Net.Clients.csproj" />
    <ProjectReference Include="..\Net.SocketIO\Net.SocketIO.csproj" />
    <ProjectReference Include="..\Serialization\Serialization.csproj" />
    <ProjectReference Include="..\UnitTesting\UnitTesting.csproj" />
    <ProjectReference Include="..\MathLight\MathLight.csproj" />
    <ProjectReference Include="..\Linq\Linq.csproj" />
    <ProjectReference Include="..\Licensing\Licensing.csproj" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net10.0' ">
    <ProjectReference Include="..\Nuget\Nuget.csproj" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' != 'net10.0' ">
    <Compile Remove="Nuget\**" />
    <EmbeddedResource Remove="Nuget\**" />
    <None Remove="Nuget\**" />
  </ItemGroup>
</Project>