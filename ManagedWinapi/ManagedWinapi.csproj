      <DependentUpon>ShortcutBox.cs</DependentUpon>
    </Compile>
    <Compile Update="TextCursorProvider.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Interop\Interop.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Crosshair.resx">
      <DependentUpon>Crosshair.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="ShortcutBox.resx">
      <DependentUpon>ShortcutBox.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Resource Include="crosshair.cur" />
    <Resource Include="crosshair.ico" />
  </ItemGroup>
