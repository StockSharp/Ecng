<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\common.props" />
  <PropertyGroup>
    <UseWindowsForms>true</UseWindowsForms>
    <IsSourcesAllow>false</IsSourcesAllow>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="$(MicrosoftWindowsCompatibilityVer)" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Accessibility\AccessibleObjectListener.cs" />
    <Compile Update="ClipboardNotifier.cs" />
    <Compile Update="Crosshair.cs" />
    <Compile Update="Crosshair.Designer.cs">
      <DependentUpon>Crosshair.cs</DependentUpon>
    </Compile>
    <Compile Update="Hooks\Hook.cs" />
    <Compile Update="Hooks\JournalHook.cs" />
