<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <StockSharpTargets>netstandard2.1;net6.0</StockSharpTargets>
    <WarningsAsErrors>true</WarningsAsErrors>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <Import Project="..\..\common.props" />

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="$(MsExtensionsVer)" />
    <PackageReference Include="MessagePipe" Version="$(MessagePipeVer)" />
    <PackageReference Include="MessagePack" Version="$(MessagePackVer)" />
    <PackageReference Include="System.IO.Pipelines" Version="$(SysPipelinesVer)" />
  </ItemGroup>
</Project>
