<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>

    <AssemblyTitle>Ecng.$(MSBuildProjectName)</AssemblyTitle>
    <Product>Ecng system framework</Product>
    <Description>Ecng system framework</Description>
    <Company>StockSharp LP</Company>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="
           '$(MSBuildProjectName)' == 'Tests'
        or '$(MSBuildProjectName)' == 'Interop.Windows'
        or '$(MSBuildProjectName)' == 'ManagedWinapi'
        or '$(MSBuildProjectName)' == 'HtmlRenderer'
        or '$(MSBuildProjectName)' == 'TestConsole'
      ">
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup Condition="
           '$(MSBuildProjectName)' == 'SmartFormat.Tests'
      ">
    <TargetFrameworks>net5.0</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(StockSharpTargets)' != ''">
    <TargetFrameworks>$(StockSharpTargets)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <ProductVersion>9.0.30729</ProductVersion>
    <RootNamespace>Ecng.$(MSBuildProjectName)</RootNamespace>
    <AssemblyName>Ecng.$(MSBuildProjectName)</AssemblyName>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <IsSourcesAllow>true</IsSourcesAllow>
    <LangVersion>default</LangVersion>

    <AWSSDKS3Ver>3.7.3.24</AWSSDKS3Ver>
    <AWSSDKSQSVer>3.7.1.34</AWSSDKSQSVer>
    <AWSSDKSimpleNotificationServiceVer>3.7.2.62</AWSSDKSimpleNotificationServiceVer>
    <AWSSDKGlacierVer>3.7.0.91</AWSSDKGlacierVer>

    <MicrosoftWindowsCompatibilityVer>5.0.2</MicrosoftWindowsCompatibilityVer>
    <MicrosoftAspNetWebApiClientVer>5.2.7</MicrosoftAspNetWebApiClientVer>

    <!-- https://github.com/dotnet/runtime/issues/61778 this issue prevents MatLab from loading this library -->
    <EcngSystemComponentModelAnnotationsVer>5.0.0</EcngSystemComponentModelAnnotationsVer>

    <SystemCodeDomVer>5.0.0</SystemCodeDomVer>
    <SystemCollectionsImmutableVer>5.0.0</SystemCollectionsImmutableVer>
    <SystemConfigurationConfigurationManagerVer>5.0.0</SystemConfigurationConfigurationManagerVer>
    <SystemIOCompressionVer>4.3.0</SystemIOCompressionVer>
    <SystemManagementVer>5.0.0</SystemManagementVer>
    <SystemReflectionEmitVer>4.7.0</SystemReflectionEmitVer>
    <SystemReflectionEmitLightweightVer>4.7.0</SystemReflectionEmitLightweightVer>
    <SystemSecurityCryptographyProtectedDataVer>5.0.0</SystemSecurityCryptographyProtectedDataVer>
    <SystemSecurityPermissionsVer>5.0.0</SystemSecurityPermissionsVer>
    <SystemTextEncodingCodePagesVer>5.0.0</SystemTextEncodingCodePagesVer>
    <SystemResourcesExtensionsVer>5.0.0</SystemResourcesExtensionsVer>
    <SystemTextJsonVer>4.7.2</SystemTextJsonVer>
    <SystemValueTupleVer>4.5.0</SystemValueTupleVer>
    <MsSourceLinkGitHubVer>1.0.0</MsSourceLinkGitHubVer>
    <NUnitVer>3.12.0</NUnitVer>
    <NUnit3TestAdapterVer>3.17.0</NUnit3TestAdapterVer>
    <FluentAssertionsVer>5.10.3</FluentAssertionsVer>
    <SystemDrawingCommonVer>5.0.3</SystemDrawingCommonVer>

    <CSharpVer>4.7.0</CSharpVer>
    <MsCodeVer>3.11.0</MsCodeVer>
    <NewtonJsonVer>13.0.1</NewtonJsonVer>
    <MsTestVer>2.2.7</MsTestVer>
    <MsTestSdkVer>17.0.0</MsTestSdkVer>

    <OctodiffVer>1.2.36</OctodiffVer>
    <MoreLinqVer>3.3.2</MoreLinqVer>
    <RestSharpVer>106.13.0</RestSharpVer>
    <NDdeVer>3.1.0</NDdeVer>
    <ILPackVer>0.1.7</ILPackVer>
    <linq2dbVer>3.5.1</linq2dbVer>
    <NitoVer>5.1.2</NitoVer>
    <DXVer>1.0.5</DXVer>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyCopyrightAttribute">
      <_Parameter1>Copyright @ StockSharp $([System.DateTime]::Now.ToString(yyyy))</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Reflection.AssemblyTrademarkAttribute">
      <_Parameter1>StockSharp</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.CLSCompliantAttribute">
      <_Parameter1>true</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Resources.NeutralResourcesLanguageAttribute">
      <_Parameter1>en-US</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>
</Project>