<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup Condition="
           '$(MSBuildProjectName)' == 'Test'
        or '$(MSBuildProjectName)' == 'UnitTesting'
      ">
    <TargetFrameworks>net5.0</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup Condition="
           '$(MSBuildProjectName)' == 'EcngTest'
        or '$(MSBuildProjectName)' == 'Interop.Windows'
        or '$(MSBuildProjectName)' == 'ManagedWinapi'
        or '$(MSBuildProjectName)' == 'TestWinForms'
        or '$(MSBuildProjectName)' == 'Xaml'
      ">
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <ProductVersion>9.0.30729</ProductVersion>
    <RootNamespace>Ecng.$(MSBuildProjectName)</RootNamespace>
    <AssemblyName>Ecng.$(MSBuildProjectName)</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <CodeAnalysisRuleSet>AllRules.ruleset</CodeAnalysisRuleSet>
    <IsSourcesAllow>false</IsSourcesAllow>
    <LangVersion>default</LangVersion>

    <AWSSDKS3Ver>3.5.7.8</AWSSDKS3Ver>
    <AWSSDKSQSVer>3.5.1.10</AWSSDKSQSVer>
    <AWSSDKSimpleNotificationServiceVer>3.5.1.33</AWSSDKSimpleNotificationServiceVer>
    <AWSSDKGlacierVer>3.5.0.61</AWSSDKGlacierVer>

    <MicrosoftWindowsCompatibilityVer>5.0.1</MicrosoftWindowsCompatibilityVer>

    <SystemCodeDomVer>5.0.0</SystemCodeDomVer>
    <SystemCollectionsImmutableVer>5.0.0</SystemCollectionsImmutableVer>
    <SystemComponentModelAnnotationsVer>5.0.0</SystemComponentModelAnnotationsVer>
    <SystemConfigurationConfigurationManagerVer>5.0.0</SystemConfigurationConfigurationManagerVer>
    <SystemDataSqlClientVer>4.8.2</SystemDataSqlClientVer>
    <SystemIOCompressionVer>4.3.0</SystemIOCompressionVer>
    <SystemManagementVer>5.0.0</SystemManagementVer>
    <SystemReflectionEmitVer>4.7.0</SystemReflectionEmitVer>
    <SystemReflectionEmitLightweightVer>4.7.0</SystemReflectionEmitLightweightVer>
    <SystemSecurityCryptographyProtectedDataVer>5.0.0</SystemSecurityCryptographyProtectedDataVer>
    <SystemSecurityPermissionsVer>5.0.0</SystemSecurityPermissionsVer>
    <SystemTextEncodingCodePagesVer>5.0.0</SystemTextEncodingCodePagesVer>
    <SystemResourcesExtensionsVer>5.0.0</SystemResourcesExtensionsVer>

    <MsCodeVer>3.8.0</MsCodeVer>
    <NewtonJsonVer>12.0.3</NewtonJsonVer>
    <MsTestVer>2.1.2</MsTestVer>
    <MsTestSdkVer>16.8.3</MsTestSdkVer>
    <SmartFormatVer>2.5.3</SmartFormatVer>
    <OctodiffVer>1.2.1</OctodiffVer>
    <MoreLinqVer>3.3.2</MoreLinqVer>
    <RestSharpVer>106.11.7</RestSharpVer>
    <NDdeVer>3.0.1</NDdeVer>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>none</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(SolutionDir)EcngAssemblyInfo.cs" Link="Properties\EcngAssemblyInfo.cs" />
  </ItemGroup>
</Project>