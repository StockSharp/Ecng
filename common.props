<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>

    <Title>Ecng.$(MSBuildProjectName)</Title>
    <Product>Ecng system framework</Product>
    <Company>StockSharp LP</Company>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="
           '$(MSBuildProjectName)' == 'EcngTest'
        or '$(MSBuildProjectName)' == 'Interop.Windows'
        or '$(MSBuildProjectName)' == 'ManagedWinapi'
        or '$(MSBuildProjectName)' == 'HtmlRenderer'
        or '$(MSBuildProjectName)' == 'Test'
      ">
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <ProductVersion>9.0.30729</ProductVersion>
    <RootNamespace>Ecng.$(MSBuildProjectName)</RootNamespace>
    <AssemblyName>Ecng.$(MSBuildProjectName)</AssemblyName>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <CodeAnalysisRuleSet>AllRules.ruleset</CodeAnalysisRuleSet>
    <IsSourcesAllow>true</IsSourcesAllow>
    <LangVersion>default</LangVersion>

    <AWSSDKS3Ver>3.7.1.21</AWSSDKS3Ver>
    <AWSSDKSQSVer>3.7.0.50</AWSSDKSQSVer>
    <AWSSDKSimpleNotificationServiceVer>3.7.2.20</AWSSDKSimpleNotificationServiceVer>
    <AWSSDKGlacierVer>3.7.0.49</AWSSDKGlacierVer>

    <MicrosoftWindowsCompatibilityVer>5.0.2</MicrosoftWindowsCompatibilityVer>

    <SystemCodeDomVer>5.0.0</SystemCodeDomVer>
    <SystemCollectionsImmutableVer>5.0.0</SystemCollectionsImmutableVer>
    <SystemComponentModelAnnotationsVer>5.0.0</SystemComponentModelAnnotationsVer>
    <SystemConfigurationConfigurationManagerVer>5.0.0</SystemConfigurationConfigurationManagerVer>
    <SystemIOCompressionVer>4.3.0</SystemIOCompressionVer>
    <SystemManagementVer>5.0.0</SystemManagementVer>
    <SystemReflectionEmitVer>4.7.0</SystemReflectionEmitVer>
    <SystemReflectionEmitLightweightVer>4.7.0</SystemReflectionEmitLightweightVer>
    <SystemSecurityCryptographyProtectedDataVer>5.0.0</SystemSecurityCryptographyProtectedDataVer>
    <SystemSecurityPermissionsVer>5.0.0</SystemSecurityPermissionsVer>
    <SystemTextEncodingCodePagesVer>5.0.0</SystemTextEncodingCodePagesVer>
    <SystemResourcesExtensionsVer>5.0.0</SystemResourcesExtensionsVer>

    <MsCodeVer>3.10.0</MsCodeVer>
    <NewtonJsonVer>13.0.1</NewtonJsonVer>
    <MsTestVer>2.2.5</MsTestVer>
    <MsTestSdkVer>16.10.0</MsTestSdkVer>
    <SmartFormatVer>2.7.0</SmartFormatVer>
    <OctodiffVer>1.2.1</OctodiffVer>
    <MoreLinqVer>3.3.2</MoreLinqVer>
    <RestSharpVer>106.12.0</RestSharpVer>
    <NDdeVer>3.1.0</NDdeVer>
    <ILPackVer>0.1.6</ILPackVer>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyCopyrightAttribute">
      <_Parameter1>Copyright @ StockSharp $([System.DateTime]::Now.ToString(yyyy))</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Reflection.AssemblyTrademarkAttribute">
      <_Parameter1>StockSharp</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.CLSCompliantAttribute">
      <_Parameter1>true</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Resources.NeutralResourcesLanguageAttribute">
      <_Parameter1>en-US</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>
</Project>